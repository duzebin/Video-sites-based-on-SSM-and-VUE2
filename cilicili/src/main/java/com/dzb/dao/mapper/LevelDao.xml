<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dzb.dao.LevelDao">


    <select id="getLevelUpExperienceByLevel" resultType="java.lang.Integer">
        SELECT experience
        FROM level
        WHERE level=#{level}
    </select>
</mapper>
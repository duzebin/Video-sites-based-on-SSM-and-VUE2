<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dzb.dao.PublicizeDao">

    <select id="getPublicizeByType" resultType="com.dzb.entity.Publicize">
        SELECT *
        FROM publicize
        WHERE type=#{type}
    </select>
</mapper>